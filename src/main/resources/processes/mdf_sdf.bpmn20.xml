<?xml version="1.0" encoding="UTF-8"?>
<definitions
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
	xmlns:activiti="http://activiti.org/bpmn"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL
		http://www.omg.org/spec/BPMN/2.0/20100501/BPMN20.xsd"
	targetNamespace="mdf_sdf">
	
	<process id="runDailyBatch" name="Process MDF and SDF">
		<startEvent id="start">
			<timerEventDefinition>
				<timeCycle>0 * * * * ?</timeCycle>
			</timerEventDefinition>
		</startEvent>
		
		<sequenceFlow targetRef="reports" sourceRef="start"/>
		
		<serviceTask id="reports" name="Reports" activiti:class="com.smi.efs.activiti.delegate.ReportsDelegate"/>
		
		<sequenceFlow targetRef="downloadEPFile" sourceRef="reports"/>
		
		<serviceTask id="downloadEPFile" name="VISA Incoming / Download EP File" activiti:class="com.smi.efs.activiti.delegate.DownloadEPFileDelegate"/>
		
		<sequenceFlow targetRef="end" sourceRef="downloadEPFile"/>
		
		<endEvent id="end"/>
	</process>
	
</definitions>